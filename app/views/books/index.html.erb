
<h1>Books</h1>
<p>Title</p>
<%= @books.each do |book| %>
 <p>Body</p>
 <span><%= book.title %></span>
 <span><%= book.body %></span>
<% end %>

<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <p>Title</p>
  <%= f.text_field :title %>
  
  <p>Body</p>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>
<% end %>

